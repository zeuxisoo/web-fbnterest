!!! 5
html
	head
		meta(charset='utf-8')
		title= siteTitle
		meta(content='#{siteTitle}', property='og:title')
		- if (facebookMeta.enable == false)
			meta(content='#{siteUrl}', property='og:url')
			meta(content='#{siteUrl}/images/favicon.png', property='og:image')
		- else
			meta(content='#{facebookMeta.url}', property='og:url')
			meta(content='https://graph.facebook.com/#{facebookId}/picture?type=large', property='og:image')
		meta(content="#{csrf}", name="csrf-token")
		link(href="/images/favicon.png", rel="shortcut icon", type="image/x-icon")
		link(href='/stylesheets/main.css', media='screen', rel='stylesheet', type='text/css')
		link(href='/stylesheets/tipTip.css', media='screen', rel='stylesheet', type='text/css')
		script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.7.0/jquery.min.js', type='text/javascript')
		script(src='/javascripts/jquery.masonry.min.js', type='text/javascript')
		script(src='/javascripts/jquery.tipTip.min.js', type='text/javascript')
		script(src='/javascripts/jquery.chosen.js', type='text/javascript')
		script(src='/javascripts/handlebars.js', type='text/javascript')
		script(src='/javascripts/handlebars.helpers.js', type='text/javascript')
		script
			var facebookAppId = "#{facebookAppId}", channelUrl = "#{siteUrl}/channel.html";
		script(src='/javascripts/default.js', type='text/javascript')
	body.index!= body
		#tiptip_holder
			#tiptip_content
				#tiptip_arrow
					#tiptip_arrow_inner